{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {"interface-name" : [true, "always-prefix"]},
    "rules": {
        "trailing-comma": [ false ],
        "no-console": false,
        "object-literal-sort-keys": false,
        "interface-name":[true, "always-prefix"],
        "no-shadowed-variable": false,
        "max-line-length": false
    },
    "rulesDirectory": []
}